{
  "name": "NEHU",
  "description": "A telegram CHAT bot",
  "logo": "https://telegra.ph/file/d06ff26bb81e868e32e6f.mp4",
  "keywords": [
    "Chatbot",
    "mongodb",
    "python",
    "pyrogram"
  ],
  "addons": [
    "heroku-postgresql"
  ],
  "buildpacks": [
    {
      "url": "heroku/python"
    }
  ],
  "repository": "https://github.com/Sumankumar81547/NEHA-X-CHATBOT",
  "env": {
    "API_ID": {
      "description": "Your Telegram API ID",
      "required": true
    },
    "API_HASH": {
      "description": "Your Telegram API hash",
      "required": true
    },
    "BOT_TOKEN": {
      "description": "A Bot token from @BotFather",
      "required": true
    },
    "MONGO_URL": {
      "description": "Get a mongodb url from https://cloud.mongodb.com.",
      "required": true
    },
    "OWNER_ID": {
      "description": "Your Telegram user ID (Owner ID)",
      "required": true
    },
    "OWNER_USERNAME": {
      "description": "Username of the bot owner",
      "required": true,
      "value": "NeHa_942"
    },
    "GPT_API": {
      "description": "Your GPT API Key from OpenAI.",
      "value": "",
      "required": true
    },
    "SUPPORT_GROUP": {
      "description": "Name of the support group",
      "required": true,
      "value": "R_I_S_I_NG"
    },
    "UPDATE_CHANNEL": {
      "description": "Name of the update channel",
      "required": true,
      "value": "pikachu_world"
    }
  }
}
